{
  "name": "wc-plugin-framework",
  "version": "5.10.0",
  "title": "WooCommerce Plugin Framework",
  "author": "SkyVerge Team",
  "homepage": "https://github.com/skyverge/wc-plugin-framework#readme",
  "repository": {
    "type": "git",
